a=int(input("Enter the number no.of rows :"))
val1=1
val2=0
inc1=2
rem=0
for i in range(1,a+1):
    for j in range (1,i+1):
         rem=j
         if(rem==1):
            print(val1,end=" ")
         elif(rem>1):
            val2=val1-rem+1
            print(val2,end=" ")
         else :
            print("Wrong input", end= " ")
    val1=val1+inc1
    inc1=inc1+1
    print( )
         
